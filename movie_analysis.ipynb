import pandas as pd
import matplotlib.pyplot as plt

# Sample movie dataset
data = {
    "Movie": ["Inception", "Avengers", "Titanic", "Matrix", "Joker"],
    "Rating": [8.8, 8.1, 7.8, 8.7, 8.5],
    "Year": [2010, 2012, 1997, 1999, 2019]
}

df = pd.DataFrame(data)

# Average rating
print("Average Rating:", df["Rating"].mean())

# Plot
plt.bar(df["Movie"], df["Rating"], color='skyblue')
plt.title("Movie Ratings")
plt.xlabel("Movie")
plt.ylabel("Rating")
plt.show()
